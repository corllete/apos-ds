		"extract-loader": "^3.1.0",
		"file-loader": "^5.0.2",
		"mocha": "^8.3.0",
		"mocha-chai-jest-snapshot": "^1.1.1",
		"postcss-loader": "^3.0.0",
		"sass-loader": "^7.3.1",
		"semver": "^5.7.1",
		"webpack": "^4.41.5",
		"webpack-cli": "^3.3.10"
	},
	"engines": {
		"node": ">=12.0.0"
	},
